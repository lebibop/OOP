<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="oop">

    <class name="oop.Model.Report" table="report">
        <id name="id_report" type="int" column="id_report" >
            <generator class="increment" />
        </id>

        <property name="month" >
            <column name="month" />
        </property>
        <property name="clients_per_month" >
            <column name="clients_per_month" />
        </property>
        <property name="free_per_month" >
            <column name="free_per_month" />
        </property>
        <property name="booked_per_month">
            <column name="booked_per_month" />
        </property>
        <property name="room" >
            <column name="room_id" />
        </property>
    </class>

</hibernate-mapping>